<?xml version="1.0" encoding="utf-8"?>
<Application xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="CORMobileApp.App">
  <Application.Resources>
        <ResourceDictionary>
            <Color x:Key="Primary">#056b99</Color>
            <Color x:Key="PrimaryDark">#056b99</Color>
            <Color x:Key="Accent">#067fc3</Color>


            <Style TargetType="NavigationPage">
                <Setter Property="BarBackgroundColor" Value="{StaticResource Primary}" />
                <Setter Property="BarTextColor" Value="White" />
            </Style>
        </ResourceDictionary>
    </Application.Resources>
</Application>
